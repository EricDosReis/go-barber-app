{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "global-require": 0,
      "class-methods-use-this": 0,
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "consistent-return": 0
    }
}
